result-view {
  match: _ (this) {
    from-property: Schedule (schedule) {
      from-output :GetSchedule (action)
    }
  }

  render {
    list-of (schedule.matches) {
      where-each (match) {
        layout-macro (match-result-summary) {
          param (match) { expression (match) }
        }
      }
    }
  }
}